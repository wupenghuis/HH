<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.hjcrm.system.mapper.DealrecordDao">

	<insert id="addDealrecord" parameterType="dealrecord">
		insert into hj_dealrecord(
		dealrecordId,resourceId,studentId,courseid,subjectid,userid,create_time,update_id,update_time,note,dr,scoretime,score,ispass
		) values(
		#{dealrecordId},#{resourceId},#{studentId},#{courseid},#{subjectid},#{userid},#{create_time},#{update_id},#{update_time},#{note},#{dr},#{scoretime},#{score},#{ispass}
		)
	</insert>

</mapper>
